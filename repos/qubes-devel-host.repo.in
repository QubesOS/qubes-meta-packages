[qubes-devel-host-current-testing]
name = Qubes Host Repository (devel testing)
#baseurl = https://yum.qubes-os.org/devel/current-testing/host/@DIST@
metalink = https://yum.qubes-os.org/devel/current-testing/host/@DIST@/repodata/repomd.xml.metalink
skip_if_unavailable=False
